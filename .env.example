# AI模型API配置
OPENAI_API_BASE="https://api.openai.com/v1"
OPENAI_API_KEY="your_openai_api_key_here"

# Ollama本地模型配置（可选）
OLLAMA_API_BASE="http://localhost:11434/v1"
# Ollama不需要API密钥

# DeepSeek API配置（可选）
DEEPSEEK_API_BASE="https://api.deepseek.com/v1"
DEEPSEEK_API_KEY="your_deepseek_api_key_here"

# Anthropic Claude API配置（可选）
ANTHROPIC_API_KEY="your_anthropic_api_key_here"

# GitHub Copilot API配置（可选，需要企业账户）
GITHUB_COPILOT_API_BASE="https://api.githubcopilot.com/v1"
GITHUB_COPILOT_API_KEY="your_github_copilot_token_here"

# Google Gemini API配置（可选）
GOOGLE_GEMINI_API_BASE="https://generativelanguage.googleapis.com/v1"
GOOGLE_GEMINI_API_KEY="your_google_gemini_api_key_here"

# Jina AI搜索API配置（用于市场信息搜索）
JINA_API_KEY="your_jina_api_key_here"

# OKX交易所API配置（用于加密货币交易）
OKX_API_KEY="your_okx_api_key_here"
OKX_API_SECRET="your_okx_api_secret_here"
OKX_PASSPHRASE="your_okx_passphrase_here"
OKX_TESTNET="true"  # 设置为 "false" 启用真实交易，强烈建议先使用测试网

# MCP服务端口配置
MATH_HTTP_PORT=8000
SEARCH_HTTP_PORT=8001
TRADE_OKX_HTTP_PORT=8004
GETPRICE_OKX_HTTP_PORT=8005

# AI代理配置
AGENT_MAX_STEP=30  # AI最大推理步数

# 初始资金配置（仅用于本地模拟）
INITIAL_CASH_USDT=10000.0

# 运行时环境配置文件路径（推荐使用绝对路径）
RUNTIME_ENV_PATH="./runtime_env.json"