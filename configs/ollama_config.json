{
  "agent_type": "BaseAgent",
  "trading_mode": "crypto_okx",
  "date_range": {
    "init_date": "2025-10-01",
    "end_date": "2025-10-21"
  },
  "models": [
    {
      "name": "ollama-llama3",
      "basemodel": "ollama/llama3",
